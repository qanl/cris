{
  "name": "cris",
  "version": "1.0.0",
  "dependencies": {
    "chai": "4.3.6"
  },
  "description": "TestCaf√© end-to-end test automation boilerplate. Using Commonjs, Chai and Expect. CRIS",
  "devDependencies": {
    "dotenv": "^16.0.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-testcafe": "0.2.1",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "minimist": "^1.2.6",
    "prettier": "2.6.0",
    "pretty-quick": "3.1.3",
    "testcafe": "^1.18.5",
    "testcafe-reporter-xunit": "^2.2.1"
  },
  "homepage": "https://github.com/qanl/cris#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "keywords": [
    "cris",
    "chai",
    "commonjs",
    "expect",
    "testcafe",
    "testcafe automation",
    "testcafe boilerplate",
    "testcafe example",
    "e2e",
    "e2e tests",
    "boilerplate",
    "integration test",
    "test automation",
    "javascript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qanl/cris.git"
  },
  "scripts": {
    "lint": "npx eslint --ext .js ./ --fix",
    "test": "npx testcafe \"chrome:headless\" --disable-page-caching spec/e2e.spec.js --reporter spec,xunit:report.xml"
  }
}
