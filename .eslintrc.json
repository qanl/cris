{
    "env": {
    "commonjs": true,
        "node": true
},
    "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "plugin:testcafe/recommended"
],
    "plugins": [
    "testcafe"
],
    "rules": {
    "no-unused-expressions": 0
},
"edit.codeActionsOnSave": {
    "source.fixAll.eslint": true
}
}