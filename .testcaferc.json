{
  "browsers": "chrome",
  "proxyBypass": "*.36eighttechnologies.com",
  "disableHttp2": false,
  "baseUrl": "https://cris-web.36eighttechnologies.com/",
  "src": ["src/spec/orders.spec.ts"],
  "reporter": [
    {
      "name": "spec"
    },
    {
      "name": "html",
      "output": "./artifacts/E2E_PROD_26_July_report.html"
    }
  ],
  "screenshots": {
    "path": "./artifacts/screenshots/",
    "takeOnFails": true,
    "fullPage": false,
    "pathPattern": "/${DATE}_${TIME}/test-${TEST_INDEX}/${USERAGENT}/${FILE_INDEX}.png"
  },
  "filter": {
    "testGrep": ""
  },
  "quarantineMode": false,
  "stopOnFirstFail": false,
  "skipJsErrors": true,
  "skipUncaughtErrors": true,
  "concurrency": 1,
  "selectorTimeout": 3000,
  "assertionTimeout": 3000,
  "pageLoadTimeout": 1000,
  "disablePageCaching": true
}
